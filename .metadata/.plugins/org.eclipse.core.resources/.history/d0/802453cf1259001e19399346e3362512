package demo;

	interface Animal{
		public void eat();
	}

	interface Dog {
		public void sleep1();
	}

	public class Interface implements Animal,Dog{
		
		public void sleep() {
			// TODO Auto-generated method stub
			System.out.println("Dog sleeping");
		}

		@Override
		public void eat() {
			// TODO Auto-generated method stub
			System.out.println("eating ...");
		}

		public static void main(String[] args) {
			// TODO Auto-generated method stub
			Interface d = new Interface();
			d.eat();
			d.sleep();
			
		}

		
	

	
		}


