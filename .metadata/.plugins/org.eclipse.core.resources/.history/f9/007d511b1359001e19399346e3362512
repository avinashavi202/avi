package demo;

	interface Animal{
		public void eat();
	}

	interface Dog {
		public void sleep1();
	}

	public class Interface implements Animal,Dog{
		
		public void barking() {
			// TODO Auto-generated method stub
			System.out.println("Dog is barking...");
		}

		@Override
		public void play() {
			// TODO Auto-generated method stub
			System.out.println("dog is playing ...");
		}

		public static void main(String[] args) {
			// TODO Auto-generated method stub
			Interface d = new Interface();
			d.eat();
			d.sleep();
			
		}

		@Override
		public void sleep1() {
			// TODO Auto-generated method stub
			
		}
	
		}


